{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "deep-link:default",
    {
      "identifier": "shell:allow-open",
      "allow": [{
        "name": "open-browser",
        "cmd": "open",
        "args": [
          "https://sean.kintone.com/**",
          "https://seanbase.com/**"
        ]
      }]
    }
  ]
}